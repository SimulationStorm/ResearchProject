config_version=5

[application]

config/name="ResearchProject"
run/main_scene="res://Core/Views/MainView.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://Assets/Icons/ProjectIcon.png"

[autoload]

App="*res://Singletons/App.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/resizable=false
window/size/borderless=true
window/per_pixel_transparency/allowed=true
mouse_cursor/custom_image_hotspot=Vector2(10, 10)

[dotnet]

project/assembly_name="ResearchProject"

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray()

